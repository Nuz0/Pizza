<Window x:Class="pizza.Reg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pizza"
        mc:Ignorable="d"
        Title="Reg" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="EmailBox" HorizontalAlignment="Left" Height="23" Margin="340,189,0,0" TextWrapping="Wrap" Text="Почта" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="passBox" HorizontalAlignment="Left" Height="23" Margin="340,217,0,0" TextWrapping="Wrap" Text="Пароль" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="RegBut" Content="Регистрация" HorizontalAlignment="Left" Margin="340,245,0,0" VerticalAlignment="Top" Width="120" Click="RegBut_Click"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Left" Margin="707,389,0,0" VerticalAlignment="Top" Width="75" Click="Back_Click"/>
        <TextBox x:Name="LogiBox" HorizontalAlignment="Left" Height="23" Margin="340,161,0,0" TextWrapping="Wrap" Text="Логин" VerticalAlignment="Top" Width="120"/>
        <Frame x:Name="Frame" Content="Frame" HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792"/>

    </Grid>
</Window>
